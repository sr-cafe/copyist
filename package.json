{
  "name": "copyist",
  "version": "1.0.0",
  "description": "Translates key=value strings to a Javascript object. Mostly designed to work with files containing copies, so it supports key duplication (think of multiple paragraphs) and multiline values.",
  "main": "index.js",
  "scripts": {
    "lint": "coffeelint ./lib/*.coffee",
    "test": "npm run lint && jasmine-node --coffee --verbose spec",
    "doc": "yuidoc ./lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sr-cafe/copyist.git"
  },
  "keywords": [
    "javascript",
    "nodejs",
    "object",
    "properties",
    "key/value"
  ],
  "author": "Jose Rubio <jose.rubio@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sr-cafe/copyist/issues"
  },
  "homepage": "https://github.com/sr-cafe/copyist",
  "devDependencies": {
    "coffeelint": "^1.6.1",
    "jasmine-expect": "^1.22.2",
    "jasmine-node": "^1.14.5",
    "yuidocjs": "^0.4.0"
  },
  "dependencies": {
    "coffee-script": "^1.8.0"
  }
}
